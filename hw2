int change = 1;
int before = 0;
int now = 0;
int btnPin = 13;







void setup() {
  display.init();  //螢幕初始化
  display.clear();  //清空畫面
  pinMode(btnPin,INPUT);
  //display.flipScreenVertically();
  Serial.begin(115200);
}


void loop() {
  Serial.println(change);
   



now=digitalRead(btnPin);
if(now==0&&before==1 ){
  change=-change;
}
before=now;



  
  if(change==1){
    display.drawXbm(0, 0, img_width, img_height, img1);  //設定圖片
    display.display();  //將圖形顯示於畫面
    delay(300);
    display.clear();  //清空畫面
    
    
  }
  else if(change==-1){  
    display.drawXbm(0, 0, img_width, img_height, img2);  //設定圖片
    display.display();  //將圖形顯示於畫面
    delay(300);
    display.clear();  //清空畫面
  
  }
  
  
  
  
  

}
